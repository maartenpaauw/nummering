    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "lint-staged": {
    "*.js": "eslint",
    "*.vue": [
      "eslint",
      "pug-lint-vue",
      "stylelint --syntax scss"
    ]
  },
  "pre-commit": "lint-staged",
  "stylelint": {
    "processors": [
      "stylelint-processor-html"
    ],
    "extends": "stylelint-config-standard",
    "rules": {
      "no-empty-source": null
    }
  }
